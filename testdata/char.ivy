# Comparison.

'123456789' == '5'
	0 0 0 0 1 0 0 0 0

'123456789' != '5'
	1 1 1 1 0 1 1 1 1

'123456789' < '5'
	1 1 1 1 0 0 0 0 0

'123456789' <= '5'
	1 1 1 1 1 0 0 0 0

'123456789' > '5'
	0 0 0 0 0 1 1 1 1

'123456789' >= '5'
	0 0 0 0 1 1 1 1 1

'\x00\x01' or '\x00'
	0 1

'\x00\x01' or '\x01'
	1 1

'\x00\x01' and '\x00'
	0 0

'\x00\x01' and '\x01'
	0 1

'\x00\x01' nor '\x00'
	1 0

'\x00\x01' nor '\x01'
	0 0

'\x00\x01' nand '\x00'
	1 1

'\x00\x01' nand '\x01'
	1 0

'\x00\x01' xor '\x00'
	0 1

'\x00\x01' xor '\x01'
	1 0

